{
  "name": "template",
  "version": "0.0.1",
  "description": "A typescript project template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "watch": "tsc --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md,html,yml}\""
  },
  "devDependencies": {
    "@bna/commitlint-config": "^1.1.0",
    "@bna/eslint-config": "^1.1.1-next.63ea221.0",
    "@commitlint/cli": "^12.1.1",
    "@types/jest": "^26.0.22",
    "husky": "4.3.8",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@bna/commitlint-config/lerna"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@bna/eslint-config"
    ]
  },
  "prettier": "@bna/prettier-config"
}
